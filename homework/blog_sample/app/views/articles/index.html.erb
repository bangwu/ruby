<table>
  <tr>
    <td>Title</td>
    <td>Text</td>
    <td>Show</td>
    <td>Edit</td>
    <td>Delete</td>
  </tr>
  <% @articles.each do |article|%>
  <tr>
    <td><%= article.title %></td>
    <td><%= article.text %></td>
    <td><%= link_to "show", article_path(article)%></td>
    <td><%= link_to "edit", edit_article_path(article)%></td>
    <td><%= link_to "delete", article_path(article), method: :delete, data: { confirm: "Are you sure delete?"}%>
  </tr>
<% end %>
</table>
<%= link_to "new", new_article_path %>
